<Window x:Class="Propertiji.Kursevi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Propertiji"
        mc:Ignorable="d"
        Title="Kursevi" Height="136" Width="800" FontSize="24">
	<StackPanel>
		<UniformGrid Columns="2">
			<Label>Naziv:</Label>
			<TextBox Text="{Binding Path=Naziv}"/>
		</UniformGrid>
		<StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
			<Button Click="Otkazi">Otkazi</Button>
			<Button Click="Unos">OK</Button>
		</StackPanel>
	</StackPanel>
</Window>
